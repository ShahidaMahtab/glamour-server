{
	"version": 2,
	"builds": [
		{
			"src": "./index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/products",
			"dest": "index.js"
		},
		{
			"src": "/order",
			"dest": "index.js"
		},
		{
			"src": "/reviews",
			"dest": "index.js"
		},
		{
			"src": "/users",
			"dest": "index.js"
		},
		{
			"src": "/admin/(.*)",
			"dest": "index.js"
		},
		{
			"src": "/payments/(.*)",
			"dest": "index.js"
		},
		{
			"src": "/(.*)",
			"dest": "index.js"
		}
	]
}
